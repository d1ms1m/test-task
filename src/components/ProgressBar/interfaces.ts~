export enum ProgressBarType {
  'CIRCLE' = 'circle',
  'DASHBOARD' = 'dashboard',
}

export enum ProgressBarStatus {
  'IN_PROGRESS' = 'in_progress',
  'SUCCESS' = 'success',
  'WARNING' = 'warning',
  'ERROR' = 'error',
}

export enum ProgressBarStatusColor {
  'in_progress' = '#3A9EFA',
  'success' = '#49CB6F',
  'warning' = '#E2A54A',
  'error' = '#F5584C',
}
